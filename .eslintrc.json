{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "prettier",
    "jest"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": 0
  },
  "globals": {
    "React": true,
    "ReactDOM": true,
    "render": true
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.10.2"
    }
  },
  "env": {
    "browser": true,
    "amd": true,
    "node": true,
    "jest": true,
    "es6": true
  }
}
